<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <servlet>
  	<description>JAX-WS endpoint - WSLoginService</description>
  	<display-name>WSLoginService</display-name>
  	<servlet-name>WSLoginService</servlet-name>
  	<servlet-class>
  		com.sun.xml.ws.transport.http.servlet.WSServlet
  	</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSLoadcommentPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSCommentPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSDownloadAvatarPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSRefreshReceivePort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSEditProfilePort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSLoadReceivePort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSPublishPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSCreateTablePort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSRegisterPort</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>WSLoginService</servlet-name>
  	<url-pattern>/WSLoginPort</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <listener>
  	<listener-class>
  		com.sun.xml.ws.transport.http.servlet.WSServletContextListener
  	</listener-class>
  </listener></web-app>
